<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
fx:controller="sample.controllers.SceneController">
      <HBox fx:id="hboxKoszyki" prefHeight="126.0" prefWidth="600.0">
            <ListView fx:id="koszyk1" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="koszyk2" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="koszyk3" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="koszyk4" prefHeight="200.0" prefWidth="200.0" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="63.0" prefWidth="600.0" spacing="8.0">
            <Button fx:id="drawButton" mnemonicParsing="false" onAction="#handleDrawButton" text="Losowanie" />
            <Button fx:id="quickDrawButton" mnemonicParsing="false" onAction="#handleQuickDrawButton" text="Szybkie losowanie" />
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#handleResetButton" text="Reset" />
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="600.0">
            <HBox prefHeight="100.0" prefWidth="200.0">
                  <ListView fx:id="grupaA" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="grupaB" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="grupaC" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="grupaD" prefHeight="200.0" prefWidth="200.0" />
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                  <ListView fx:id="grupaE" prefHeight="200.0" prefWidth="200.0" />
                  <ListView prefHeight="200.0" prefWidth="200.0" />
                  <ListView prefHeight="200.0" prefWidth="200.0" />
            </HBox>
      </VBox>
</VBox>
