<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.SceneController">
      <HBox fx:id="hboxKoszyki" alignment="CENTER" prefHeight="126.0" prefWidth="600.0" spacing="10.0">
            <ListView fx:id="koszyk1" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="koszyk2" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="koszyk3" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="koszyk4" prefHeight="200.0" prefWidth="200.0" />
      <VBox.margin>
