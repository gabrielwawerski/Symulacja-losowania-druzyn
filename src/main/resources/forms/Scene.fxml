<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.SceneController">
      <HBox fx:id="hboxKoszyki" alignment="CENTER" prefHeight="126.0" prefWidth="600.0" spacing="10.0">
            <ListView fx:id="basket1" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="basket2" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="basket3" prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="basket4" prefHeight="200.0" prefWidth="200.0" />
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="63.0" prefWidth="600.0" spacing="8.0">
            <Button fx:id="drawButton" mnemonicParsing="false" onAction="#handleDrawButton" prefHeight="50.0" prefWidth="150.0" text="Losowanie" />
            <Button fx:id="quickDrawButton" mnemonicParsing="false" onAction="#handleQuickDrawButton" prefHeight="50.0" prefWidth="150.0" text="Szybkie losowanie" />
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#handleResetButton" prefHeight="50.0" prefWidth="150.0" text="Reset" />
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="600.0">
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                  <ListView fx:id="groupA" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="groupB" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="groupC" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="groupD" prefHeight="200.0" prefWidth="200.0" />
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                  <ListView fx:id="groupE" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="groupF" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="groupG" prefHeight="200.0" prefWidth="200.0" />
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </padding>
            </HBox>
      </VBox>
</VBox>
