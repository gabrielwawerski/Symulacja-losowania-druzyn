         </VBox.margin>
      </HBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="600.0">
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                  <ListView fx:id="grupaA" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="grupaB" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="grupaC" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="grupaD" prefHeight="200.0" prefWidth="200.0" />
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
